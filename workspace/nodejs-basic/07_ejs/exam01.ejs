<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
	</head>
	<body>
		<table>
			<tr>
				<th>번호</th>
				<th>제목</th>
				<th>작성자</th>
			</tr>
			<%
			for (var i = 1; i < 5; i++) {
			%>
				<tr><td><%= i %></td><td>제목<%= i %></td><td>작성자<%= i %></td></tr>
			<% } %>
		</table>
	</body>
	
</html>
