<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
</head>
<body>
	<h1>node board</h1>
		<table>
		<tr>
			<th>제목</th>
			<th>글쓴이</th>
			<th>내용</th>
		</tr>
		<%
		rows.forEach(function (row, i) {
		%>
		<tr>
			<td><%= row.no%></td>
			<td><%= row.title%></td>
			<td><%= row.writer%></td>
		</tr>
		<% }); %>
		</table>
</body>
</html>